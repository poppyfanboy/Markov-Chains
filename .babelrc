{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "modules": "auto",
                "corejs": 3,
                "loose": false
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "ignore": ["public/*"],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "@model": "./src/model",
                    "@components": "./src/components"
                }
            }
        ]
    ]
}
